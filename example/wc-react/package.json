{
  "name": "react",
  "version": "1.0.0",
  "description": "En Web Component implementert med React og Redux.",
  "main": "index.js",
  "scripts": {
    "stats": "cross-env NODE_ENV=development webpack --config webpack.dev.js --profile --json > stats.json",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js --display-reasons --progress --profile --color --hide-modules --display-optimization-bailout",
    "analyze_build": "cross-env NODE_ENV=analyze webpack --config webpack.prod.js --display-reasons --progress --profile --color --hide-modules --display-optimization-bailout",
    "tbv": "npm run prettier && npm run lint && npm test && npm run build && npm run owasp_check",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.js --open --port 3003",
    "backend": "node backend/index.js",
    "backend_utvikling": "cross-env LOGG_WEBSERVICE_URL=http://puma-logg-webservice.kpt.spk.no node backend/index.js",
    "test": "mocha -r @babel/register -r mocha.setup.js \"./tests/**/*_test.js\" --exit",
    "test_file": "mocha -r @babel/register -r mocha.setup.js --timeout 120000 --exit",
    "tdd": "npm run test -- --watch",
    "lint": "eslint app/ backend/ tests/ -c .eslintrc --ext .js --ext .jsx --fix --max-warnings 0",
    "prettier": "npm run prettierFrontend && npm run prettierBackend",
    "prettierFrontend": "prettier --write --trailing-comma=es5 --bracket-spacing --single-quote --tab-width=4 \"app/**/*.{js,jsx}\"",
    "prettierBackend": "prettier --write --bracket-spacing --single-quote --tab-width=4 \"backend/**/*.js\"",
    "owasp_check": "retire --path . --severity critical"
  },
  "keywords": [
    "web",
    "react",
    "redux",
    "webpack",
    "express"
  ],
  "license": "ISC",
  "devDependencies": {
